{
  "name": "eleventy-test",
  "version": "0.1.0",
  "description": "Multi-setup testing for Eleventy plugins",
  "scripts": {
    "start": "node dist/index.js",
    "watch": "yarn build --watch",
    "build": "esbuild index.ts --bundle --outfile=dist/index.js --platform=node",
    "postbuild": "tsc --declaration index.ts --emitDeclarationOnly --outDir ./dist",
    "test": "ava tests/tests/everything-builds.mjs --timeout=30s"
  },
  "repository": "https://github.com/Denperidge/eleventy-test.git",
  "author": "Denperidge",
  "license": "MIT",
  "devDependencies": {
    "ava": "^6.2.0",
    "esbuild": "^0.24.0",
    "jsdom": "^25.0.1",
    "typescript": "^5.7.2"
  },
  "dependencies": {},
  "optionalDependencies": {
    "@11ty/eleventy1": "npm:@11ty/eleventy@1.0.2",
    "@11ty/eleventy2": "npm:@11ty/eleventy@2.0.1",
    "@11ty/eleventy3": "npm:@11ty/eleventy@3.0.0"
  }
}
